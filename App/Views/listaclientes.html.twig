        </section>
        <nav class="navbar" id="nav">
            <button class="cerrarmenu" id="cerrar"><i class="fa-solid fa-right-from-bracket fa-xl"
                                                    style="color: #ffffff;"></i></button>
            <div class="menu1">
                <ul class="listmenu1">
                    <li><a href="/" class="inicioa">Inicio</a></li>
                    <li><a href="/horarios" class="inicioa">Horarios</a></li>
                    <li><a href="/planes" class="inicioa">Planes</a></li>
                    <li><a href="/">Nosotros</a></li>
                </ul>
            </div>
            <div class="menu2">
                <ul class="listmenu2" id="welcome-message">
                </ul>
            </div>
        </nav>
    </header>

<section class="listaclientes">
    <table class="lista-clientes" id="tablaClientes">
        <tbody>
            <tr>
                <th> Nombre de Cliente </th>
                <th> Documento </th>
                <th> Rol </th>
                <th> Ficha </th>
            </tr>
        </tbody>
    </table>
</section>
<div class="fichagnrl" id="fichagnl">
    <div class="fichaTecnica">
        <section class="headFicha">
                <h4>Ficha técnica de </h4>
            <button class="cerrarficha" id="cerrarficha" data-cliente-id="">
                <i class="fa-solid fa-xmark fa-2xl" style="color: #ffffff;"></i>
            </button>
        </section>
    <div class="divficha-container">
                <div class="imgCliente"> 
                    <img src="../../images/clienteEjm.png"/>
                </div>
        <div class="divficha">
            <p>Documento: </p>
            <p>Edad: </p>
            <p>Email: </p>
            <p>Teléfono: </p>
            <p>Dirección: </p>
        </div>
        <div class="divficha2">
            <p>Patologías: </p>
            <p>Altura: </p>
            <p>Peso: </p>
        </div>
    </div>
            <div class="btnficha">
                <form method="get" action="calificacion.html.twig">
                    <input type="hidden" name="documento" value="">
                    <button class="btnGestionar" type="submit" class="">Gestionar</button>
                </form>

            </div>
    </div>
</div>

